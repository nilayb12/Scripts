PowerShell -NoProfile -ExecutionPolicy Bypass -Command "& {Add-Type -AssemblyName Microsoft.VisualBasic; $title = 'Residual Logs Deletion'; $msg = 'Enter the Root Path:'; $inp = [Microsoft.VisualBasic.Interaction]::InputBox($msg, $title); if ($inp -eq \"\") { exit }; $delcmd = Get-ChildItem -Path $inp -File -Include .trashed*.*, +91*.*, 91*.*, Unknown*.*, _unexpected, _F74, desktop.ini -Recurse; $delcmd | Out-File -FilePath $inp\DelOutput.txt; $delcmd | Remove-Item -Force -Verbose;}"